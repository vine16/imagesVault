<h1 class="mt-5 mb-4 header">Gallery</h1>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Image</th>
      <th scope="col">Caption</th>
    </tr>
  </thead>
  <tbody>
    <% images.forEach((image)=> { %>
    <tr>
      <td>
        <img src="<%= image.path %>" style="height: 300px" />
      </td>
      <td><%= image.caption %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
